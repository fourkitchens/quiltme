type userSubmission {
  email: String
  originalImage: String
  backgroundImage: String
  processedImage1: String
  processedImage2: String
  processedImage3: String
  processedImage4: String
}

input userSubmissionInput {
  email: String!
  image: String!
  background: String
}

extend type Mutation {
  createSubmission(data: userSubmissionInput): userSubmission
}
